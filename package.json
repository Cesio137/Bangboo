    "rou3": "^0.7.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.18.10",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "imports": {
    "#env": [
      "./build/env.js"
    ],
    "#base": [
      "./build/discord/base/index.js"
